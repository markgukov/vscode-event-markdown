{
    "name": "emd",
    "displayName": "Event Markdown",
    "description": "Support for Event Markdown syntax",
    "version": "0.0.4",
    "publisher": "markgukov",
    "author": {
        "name": "Mark Gukov",
        "url": "http://markgukov.com"
    },
    "repository": "https://github.com/markgukov/vscode-event-markdown",
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "emd",
            "aliases": ["Event Markdown", "emd"],
            "extensions": [".emd"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "emd",
            "scopeName": "text.html.emd",
            "path": "./syntaxes/emd.tmLanguage.json"
        }],
		"themes": [{
            "label": "EMD",
            "uiTheme": "vs-dark",
            "path": "./themes/emd-color-theme.json"
        }]
    }
}